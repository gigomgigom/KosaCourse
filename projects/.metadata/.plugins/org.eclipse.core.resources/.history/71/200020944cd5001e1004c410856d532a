package ch02.sec06;

public class TextBlockExample {
	
	/*
	{
		"id": "윈터",
		"name": "눈송이"
	}
	*/

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//우리가 일전에 배웠던 방식대로 한번 작성해보자
		String str1 = "" + "{\n" + "\t\"id\": \"윈터\"\n" + "}";
		
		System.out.println(str1);
	}

}
