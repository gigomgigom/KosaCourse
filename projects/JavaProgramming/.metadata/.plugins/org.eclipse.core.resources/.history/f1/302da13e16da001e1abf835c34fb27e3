package ch05.sltn;

public class Sltn7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] array = {1, 5, 3, 8, 2};
		int max = 0;
		
		for(int i = 0; i < array.length; i++) {
			if(max < array[i]) {
				max = array[i];
			}
		}
		System.out.println("최대값은 " + max);
	}

}
