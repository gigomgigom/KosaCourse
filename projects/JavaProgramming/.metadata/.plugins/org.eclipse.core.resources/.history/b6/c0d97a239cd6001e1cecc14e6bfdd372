package ch04.sec02;

public class IfNestedExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int score = (int)(Math.random()*20) + 81;
		System.out.println("score: " + score);
		
		String grade;
		if(score>=90) {
			if(score>=95) {
				grade = "A";
			}
			grade = "B";
		} else {
			if(score>=85) {
				grade = "C";
			} else {
				grade = "D";
			}
		}
		
		System.out.println("grade : " + grade);
	}

}
